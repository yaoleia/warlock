{
  "env": {
    "development": {
      "plugins": [
        "dynamic-import-node"
      ]
    },
    "node": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "syntax-dynamic-import"
      ]
    },
    "web": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "last 2 versions",
                "safari >= 7"
              ]
            }
          }
        ]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "syntax-dynamic-import",
        "transform-object-assign",
        [
          "transform-runtime",
          {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
          }
        ]
      ]
    }
  },
  "comments": false
}